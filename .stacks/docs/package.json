{
  "name": "stacks-docs",
  "type": "module",
  "version": "0.51.0",
  "packageManager": "pnpm@7.28.0",
  "description": "The Stacks documentation.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/docs"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "documentation",
    "vite",
    "composables",
    "headless",
    "api",
    "ssg",
    "pwa",
    "modern",
    "typescript"
  ],
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "engines": {
    "node": ">=v18.14.2",
    "pnpm": ">=7.28.0"
  },
  "scripts": {
    "dev": "pnpm run copy-src && vitepress dev src",
    "build": "pnpm run copy-src && vitepress build",
    "preview": "pnpm run copy-src && vitepress preview",
    "deploy": "pnpm run copy-src && wip",
    "copy-src": "rimraf ./src/* && cp -R ../../docs/* ./src"
  },
  "dependencies": {
    "@stacksjs/docs": "workspace:*",
    "@stacksjs/path": "workspace:*",
    "rimraf": "^4.1.2",
    "vitepress": "^1.0.0-alpha.48"
  }
}
