{
  "name": "stacks-init",
  "type": "module",
  "version": "0.36.6",
  "packageManager": "pnpm@7.13.6",
  "description": "Stacks CLI source.",
  "author": "Chris Breuer",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/cli#readme",
