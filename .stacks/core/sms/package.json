    "@stacksjs/error-handling": "workspace:*",
    "@stacksjs/types": "workspace:*"
  },
  "optionalDependencies": {
    "@novu/gupshup": "^0.20.0",
    "@novu/nexmo": "^0.20.0",
    "@novu/node": "^0.20.0",
    "@novu/plivo": "^0.20.0",
    "@novu/sms77": "^0.20.0",
    "@novu/sns": "^0.20.0",
    "@novu/stateless": "^0.20.0",
    "@novu/telnyx": "^0.20.0",
    "@novu/termii": "^0.20.0",
    "@novu/twilio": "^0.20.0"
  },
  "devDependencies": {
    "@stacksjs/config": "workspace:*",
    "@stacksjs/development": "workspace:*"
  }
}
