{
  "name": "@stacksjs/cloud",
  "type": "module",
  "version": "0.57.4",
  "description": "The Stacks cloud/serverless integration & implementation.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/cloud#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/core/cloud"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "cloud",
    "serverless",
    "infrastructure as code",
    "automation",
    "aws",
    "google cloud",
    "stacks"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "bun": "./src/*",
      "import": "./dist/*"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "bootstrap": "cdk bootstrap --profile stacks",
    "deploy": "bunx vitepress build ../../docs && cdk deploy --profile stacks --require-approval never",
    "build": "bun build.ts",
    "build-layer": "bun ./src/runtime/aws/scripts/build-layer.ts",
    "publish-layer": "bun ./src/runtime/aws/scripts/publish-layer.ts",
    "typecheck": "bun --bun tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "@stacksjs/dns": "workspace:*",
    "aws-cdk-lib": "^2.95.1",
    "constructs": "^10.2.70"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.12.1",
    "@aws-lambda-powertools/metrics": "^1.12.1",
    "@aws-lambda-powertools/tracer": "^1.12.1",
    "@aws-sdk/client-cloudfront": "^3.409.0",
    "@stacksjs/dns": "workspace:*",
    "aws-cdk-lib": "^2.95.1",
    "aws4fetch": "^1.0.17",
    "constructs": "^10.2.70"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*",
    "@stacksjs/env": "workspace:*",
    "jszip": "^3.10.1",
    "oclif": "^3.15.0"
  }
}
