{
  "name": "@stacksjs/vite",
  "type": "module",
  "version": "0.57.4",
  "description": "The Stacks Vite plugin.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/vite-plugin#readme",
  "repository": {
    "type": "git",
