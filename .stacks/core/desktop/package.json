{
  "name": "@stacksjs/desktop",
  "type": "module",
  "version": "0.57.4",
  "description": "The Stacks Desktop engine.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/desktop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/core/desktop"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "desktop",
    "nexmo",
    "twilio",
    "aws",
    "sms",
    "email",
    "database",
    "slack",
    "discord",
    "webhooks",
    "stacks",
    "Maizzle",
    "maizzle"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "bun": "./src/*",
      "import": "./dist/*"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun --bun build.ts",
    "build:app": "vite build ../../../../storage/framework/desktop/dashboard -c ../../core/vite/src/desktop.ts",
    "dev": "bun --bun vite serve ../../../storage/framework/desktop/dashboard -c ../../core/vite/src/desktop.ts",
    "dev:app": "tauri dev",
    "typecheck": "bun --bun tsc --noEmit",
    "prepublishOnly": "bun --bun run build"
  },
  "peerDependencies": {
    "@stacksjs/build": "workspace:*",
    "@stacksjs/server": "workspace:*",
    "@tauri-apps/api": "1.5.0",
    "@tauri-apps/cli": "1.5.0",
    "unified-network": "0.6.4",
    "unstorage": "^1.9.0",
    "vue": "3.3.4",
    "vue3-highlightjs": "^1.0.5"
  },
  "dependencies": {
    "@stacksjs/build": "workspace:*",
    "@stacksjs/server": "workspace:*",
    "@tauri-apps/api": "1.5.0",
    "@tauri-apps/cli": "1.5.0",
    "unified-network": "0.6.4",
    "unstorage": "^1.9.0",
    "vue": "3.3.4",
    "vue3-highlightjs": "^1.0.5"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*"
  }
}
