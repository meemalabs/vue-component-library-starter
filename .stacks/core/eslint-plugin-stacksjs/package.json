{
  "name": "eslint-plugin-stacksjs",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/eslint-plugin-stacksjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunx unbuild",
    "build-bun": "bun --bun build.ts",
    "stub": "bunx unbuild --stub",
    "test": "bun --bun test",
    "prepublishOnly": "bun --bun run build"
  },
  "dependencies": {
    "@stacksjs/eslint-config": "workspace:*",
    "@typescript-eslint/utils": "^6.8.0"
  },
  "devDependencies": {
    "unbuild": "latest"
  }
}
