{
  "name": "@stacksjs/docs",
  "type": "module",
  "version": "0.57.4",
  "description": "The Stacks way to document.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/docs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/core/docs"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "docs",
    "utilities",
    "functions",
    "stacks"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "bun": "./src/*",
      "import": "./dist/*"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev:docs": "bunx vitepress dev",
    "build:docs": "bunx rimraf ./node_modules && bunx vitepress build",
    "preview:docs": "bunx vitepress preview",
    "build": "bun --bun build.ts",
    "typecheck": "bun --bun tsc --noEmit",
    "prepublishOnly": "bun --bun run build"
  },
  "peerDependencies": {
    "@stacksjs/alias": "workspace:*",
    "@stacksjs/config": "workspace:*",
    "@stacksjs/path": "workspace:*",
    "@stacksjs/server": "workspace:*",
    "@stacksjs/vite": "workspace:*",
    "vitepress": "1.0.0-rc.11"
  },
  "dependencies": {
    "@stacksjs/alias": "workspace:*",
    "@stacksjs/config": "workspace:*",
    "@stacksjs/path": "workspace:*",
    "@stacksjs/server": "workspace:*",
    "@stacksjs/vite": "workspace:*",
    "vitepress": "1.0.0-rc.11"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*"
  }
}
