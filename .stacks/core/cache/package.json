{
  "name": "@stacksjs/cache",
  "type": "module",
  "version": "0.54.5",
  "packageManager": "pnpm@8.6.2",
  "description": "The Stacks way of caching.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/cache#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/core/cache"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "cache",
    "serverless",
    "redis",
    "upstash",
    "dynamodb",
    "utilities",
    "functions",
    "stacks"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./drivers/dynamodb": {
      "types": "./dist/drivers/dynamodb.d.ts",
      "import": "./dist/drivers/dynamodb.mjs"
    },
    "./drivers/memcached": {
      "types": "./dist/drivers/dynamodb.d.ts",
      "import": "./dist/drivers/dynamodb.mjs"
    },
    "./drivers/redis": {
      "types": "./dist/drivers/dynamodb.d.ts",
      "import": "./dist/drivers/dynamodb.mjs"
    },
    "./drivers/upstash": {
      "types": "./dist/drivers/dynamodb.d.ts",
      "import": "./dist/drivers/dynamodb.mjs"
    }
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.350.0",
    "@stacksjs/config": "workspace:*",
    "@types/memjs": "^1.3.0",
    "memcached": "^2.2.2",
    "memjs": "^1.3.1",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*"
  }
}
