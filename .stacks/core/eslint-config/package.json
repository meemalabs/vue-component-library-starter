{
  "name": "@stacksjs/eslint-config",
  "type": "module",
  "version": "0.57.4",
  "description": "The Stacks.js ESLint config preset, heavily inspired by Anthony's ESLint config.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com> (https://github.com/antfu/)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/stacksjs/stacks/blob/main/.stacks/core/eslint-config",
  "keywords": [
    "stacks",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunx --bun tsup --format esm,cjs --clean --dts",
    "stub": "bunx --bun tsup --format esm",
    "dev": "bunx --bun tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "bun run stub && eslint .",
    "prepack": "bun run build",
    "test": "bunx vitest",
    "typecheck": "bunx tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=8.54.0"
  },
  "dependencies": {
    "@antfu/eslint-define-config": "^1.23.0-2",
    "@eslint-types/jsdoc": "46.9.0",
    "@eslint-types/typescript-eslint": "^6.12.0",
    "@eslint-types/unicorn": "^49.0.0",
    "@stacksjs/cli": "workspace:*",
    "@stylistic/eslint-plugin": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint-config-flat-gitignore": "^0.1.1",
    "eslint-plugin-antfu": "^1.0.10",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.4.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.10",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-yml": "^1.10.0",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.3.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@stacksjs/eslint-config": "workspace:*",
    "@stacksjs/utils": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^1.4.1",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.9.5",
    "eslint": "^8.54.0",
    "eslint-flat-config-viewer": "^0.1.3",
    "lint-staged": "^15.1.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
