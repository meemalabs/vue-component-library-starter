{
  "name": "@stacksjs/eslint-config",
  "type": "module",
  "version": "0.57.4",
  "description": "The Stacks.js ESLint config preset, heavily inspired by Anthony's ESLint config.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com> (https://github.com/antfu/)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/stacksjs/stacks/blob/main/.stacks/core/eslint-config",
  "keywords": [
    "stacks",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunx --bun tsup src/index.ts --format esm,cjs --clean",
    "build-with-dts": "bunx --bun tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "bunx tsup src/index.ts --format esm,cjs",
    "dev": "bunx tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "bun run stub && eslint .",
    "prepack": "bun run build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=8.53.0"
  },
  "dependencies": {
    "@antfu/eslint-define-config": "^1.23.0-2",
    "@stylistic/eslint-plugin": "1.0.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-flat-gitignore": "^0.1.1",
    "eslint-plugin-antfu": "^1.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.9",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-yml": "^1.10.0",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.3.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*",
    "@stacksjs/eslint-config": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^1.0.1",
    "@types/eslint": "^8.44.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.9.0",
    "eslint": "^8.53.0",
    "eslint-flat-config-viewer": "^0.1.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "sucrase": "^3.34.0"
  }
}
