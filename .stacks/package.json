{
  "name": "@ow3/stacks",
  "type": "module",
  "version": "0.23.0",
  "private": true,
  "packageManager": "pnpm@7.9.5",
  "description": "The Stacks Framework.",
  "author": "Chris Breuer",
  "license": "MIT",
  "homepage": "https://github.com/ow3org/stacks/tree/main/.stacks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ow3org/stacks.git",
    "directory": "./.stacks"
  },
  "bugs": {
    "url": "https://github.com/ow3org/stacks/issues"
  },
  "keywords": [
    "framework",
    "typescript",
    "javascript",
    "vite",
    "nitro",
    "vue",
    "functions",
    "components",
    "library",
    "starter",
    "kit"
  ],
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "artisan": "./dist/cli/artisan.mjs"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "engines": {
    "node": ">=v16.17.0",
    "pnpm": ">=7.9.5"
  },
  "scripts": {
    "artisan": "jiti ./src/cli/index.ts",
    "dev:components": "vite serve ./src/components -c ./src/config/components.ts",
    "build:components": "vite build -c ./src/config/components.ts",
    "build:elements": "vite build -c ./src/config/elements.ts",
    "examples:vue": "pnpm run build:components && vite serve ./examples/vue -c ./src/config/example-vue.ts",
    "build:functions": "vite build -c ./src/config/functions.ts",
    "build:cli": "vite build -c ./src/config/cli.ts",
    "build:stacks": "vite -c ./src/config/stacks.ts needed for our starter kits",
    "nitro:build": "nitro build",
    "make:component": "wip",
    "make:function": "wip",
    "make:stack": "wip",
    "fresh": "pnpm run clean && pnpm i",
    "update": "pnpm --filter ./.stacks run update",
    "clean": "cd .. && rimraf .stacks/node_modules apps/**/node_modules node_modules/ pnpm-lock.yaml",
    "dev:pages": "nitro dev",
    "dev:functions": "wip",
    "dev:stacks": "wip",
    "build": "pnpm run typecheck && pnpm run build:components && pnpm run build:functions && pnpm run build:cli",
    "server:functions": "wip",
    "server:pages": "nitro build",
    "lint": "eslint . && pnpm -r --parallel lint",
    "lint:fix": "eslint . --fix && pnpm -r --parallel lint:fix",
    "commit": "cd .. && git cz",
    "release": "cd .. && bumpp .stacks/package.json apps/**/package.json package.json --execute 'pnpm run changelog' --all",
    "changelog": "cd .. && conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "size": "esno .stacks/scripts/size.ts",
    "test": "cd .. && pnpm run test:unit && pnpm run test:e2e",
    "test:e2e": "cd .. && cypress open",
    "test:unit": "cd .. && vitest",
    "test:coverage": "cd .. && vitest run --coverage",
    "test:types": "vue-tsc --noEmit",
    "types:fix": "esno scripts/fix-types.ts",
    "preinstall": "esno scripts/check-node-version.ts",
    "postinstall": "npx simple-git-hooks ../config/git.ts"
  },
  "dependencies": {
    "@babel/types": "^7.18.13",
    "@iconify-json/heroicons-outline": "^1.1.4",
    "@iconify-json/heroicons-solid": "^1.1.5",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@ow3/eslint-config": "^0.27.7",
    "@unocss/preset-icons": "^0.45.13",
    "@unocss/transformer-compile-class": "^0.45.13",
    "@vueuse/core": "^9.1.1",
    "bumpp": "^8.2.1",
    "cac": "^6.7.14",
    "change-case": "^4.1.2",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "cypress": "^10.7.0",
    "cz-git": "^1.3.11",
    "eslint": "^8.23.0",
    "eslint-plugin-cypress": "^2.12.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "lint-staged": "^13.0.3",
    "nitropack": "^0.4.24",
    "pathe": "^0.3.5",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.8.2",
    "unocss": "^0.45.13",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vite-plugin-inspect": "^0.6.0",
    "vitest": "^0.22.1",
    "vue": "^3.2.38",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^0.40.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.6",
    "@types/prompts": "^2.0.14",
    "@unocss/vite": "^0.45.13",
    "@vitejs/plugin-vue": "^3.0.3",
    "detect-indent": "^7.0.0",
    "detect-newline": "^4.0.0",
    "events": "^3.3.0",
    "jiti": "^1.14.0",
    "vitepress": "1.0.0-alpha.13"
  }
}
