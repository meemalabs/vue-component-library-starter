{
  "name": "@ow3/stacks",
  "type": "module",
  "version": "0.23.2",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "description": "The Stacks Framework.",
  "author": "Chris Breuer",
  "license": "MIT",
  "homepage": "https://github.com/ow3org/stacks/tree/main/.stacks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ow3org/stacks.git",
    "directory": "./.stacks"
  },
  "bugs": {
    "url": "https://github.com/ow3org/stacks/issues"
  },
  "keywords": [
    "framework",
    "typescript",
    "javascript",
    "vite",
    "nitro",
    "vue",
    "functions",
    "components",
    "library",
    "starter",
    "kit"
  ],
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "artisan": "./dist/cli/artisan.mjs"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "engines": {
    "node": ">=v16.17.0",
    "pnpm": ">=7.11.0"
  },
  "scripts": {
    "artisan": "jiti ./src/cli/index.ts",
    "dev:components": "vite serve ./src/components -c ./src/config/components.ts",
    "build:components": "vite build -c ./src/config/components.ts",
    "build:elements": "vite build -c ./src/config/elements.ts",
    "examples:vue": "pnpm run build:components && vite serve ./examples/vue -c ./src/config/example-vue.ts",
    "build:functions": "vite build -c ./src/config/functions.ts",
    "build:artisan-cli": "vite build -c ./src/config/cli.ts",
    "build:stacks": "vite build -c ./src/config/stacks.ts",
    "nitro:build": "nitro build",
    "make:component": "pnpm artisan make:component",
    "make:function": "pnpm artisan make:function",
    "make:lang": "pnpm artisan make:lang",
    "fresh": "pnpm run clean && pnpm i",
    "make:stack": "pnpm artisan make:stack",
    "update": "pnpm update",
    "build": "pnpm run test:types && pnpm artisan build",
    "clean": "rimraf ./node_modules ../apps/**/node_modules ../node_modules/ ../pnpm-lock.yaml",
    "dev:docs": "vitepress dev ../apps/site",
    "build:docs": "vitepress build ../apps/site",
    "dev:pages": "nitro dev",
    "server:functions": "wip",
    "server:pages": "nitro build",
    "lint": "eslint ..",
    "lint:fix": "eslint .. --fix",
    "commit": "cd .. && git cz",
    "release": "cd .. && bumpp .stacks/package.json apps/**/package.json package.json --execute 'pnpm run changelog' --all",
    "changelog": "cd .. && conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "size": "esno src/scripts/size.ts",
    "test": "vitest --config vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "test:types": "vue-tsc --noEmit",
    "types:fix": "esno src/scripts/fix-types.ts"
  },
  "dependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@ow3/eslint-config": "^0.28.2",
    "@rollup/pluginutils": "^4.2.1",
    "@unocss/vite": "^0.45.18",
    "@unocss/preset-icons": "^0.45.18",
    "@vitejs/plugin-vue": "^3.1.0",
    "@unocss/transformer-compile-class": "^0.45.18",
    "@vueuse/core": "^9.2.0",
    "bumpp": "^8.2.1",
    "cac": "^6.7.14",
    "change-case": "^4.1.2",
    "collect.js": "^4.34.3",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "cz-git": "^1.3.11",
    "debug": "^4.3.4",
    "degit": "^2.8.4",
    "eslint": "^8.23.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "lint-staged": "^13.0.3",
    "local-pkg": "^0.4.2",
    "minimatch": "^5.1.0",
    "nitropack": "^0.5.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.8.2",
    "unocss": "^0.45.18",
    "unplugin": "^0.9.5",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.1.0",
    "vite-plugin-inspect": "^0.7.1",
    "vitest": "^0.23.1",
    "vue": "^3.2.39",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^0.40.13"
  },
  "devDependencies": {
    "@babel/types": "^7.19.0",
    "@iconify-json/heroicons-outline": "^1.1.4",
    "@iconify-json/heroicons-solid": "^1.1.5",
    "@jsdevtools/ez-spawn": "^3.0.4",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.7.15",
    "@types/prompts": "^2.0.14",
    "consola": "^2.15.3",
    "detect-indent": "^7.0.0",
    "detect-newline": "^4.0.0",
    "events": "^3.3.0",
    "jiti": "^1.15.0",
    "vitepress": "1.0.0-alpha.13"
  }
}
