{
  "name": "@ow3/hello-world-stack",
  "version": "0.22.1",
  "packageManager": "pnpm@7.4.0",
  "description": "A Component Library source.",
  "author": "Chris Breuer",
  "license": "MIT",
  "homepage": "https://github.com/openwebstacks/stacks-framework/tree/main/components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openwebstacks/stacks-framework.git",
    "directory": "components"
  },
  "bugs": {
    "url": "https://github.com/openwebstacks/stacks-framework/issues"
  },
  "keywords": [
    "component",
    "library",
    "vue",
    "vite",
    "typescript",
    "javascript",
    "stack",
    "starter",
    "kit"
  ],
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=v16.15.1",
    "pnpm": ">=7.4.0"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "stub": "unbuild --stub"
  },
  "devDependencies": {
    "stacks": "workspace:*"
  },
  "build": {
    "entries": [
      "./index"
    ],
    "clean": true,
    "declaration": true,
    "rollup": {
      "emitCJS": true,
      "inlineDependencies": true
    }
  }
}
