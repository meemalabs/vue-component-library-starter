    "@vueuse/core": "^10.9.0",
    "@vueuse/head": "^2.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.31",
    "@intlify/unplugin-vue-i18n": "^3.0.1",
    "@types/markdown-it-link-attributes": "^3.0.4",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue-macros/volar": "^0.18.16",
