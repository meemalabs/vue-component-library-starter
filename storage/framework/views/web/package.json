{
  "name": "stacks-web",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bunx --bun vite-ssg build -c ../../core/vite-config/src/views.ts",
    "dev": "bunx --bun vite serve -c ../../core/vite-config/src/views.ts",
    "lint": "bunx --bun eslint .",
    "preview": "bunx --bun vite preview",
    "typecheck": "bunx --bun vue-tsc --noEmit",
    "up": "bunx taze major -I",
    "sizecheck": "bunx vite-bundle-visualizer"
  },
  "dependencies": {
    "@stacksjs/alias": "workspace:*",
    "@stacksjs/config": "workspace:*",
    "@stacksjs/server": "workspace:*",
