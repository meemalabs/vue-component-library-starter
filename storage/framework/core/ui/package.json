    "@stacksjs/config": "workspace:*",
    "@stacksjs/router": "workspace:*",
    "@unhead/vue": "^1.11.6",
    "pinia": "^2.2.2",
    "unhead": "^1.11.6",
    "unocss": "0.62.4",
    "unocss-preset-primitives": "0.0.2-beta.1",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue": "^3.5.8",
    "vue-tsc": "^2.1.6"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*"
  }
