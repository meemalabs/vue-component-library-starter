{
  "name": "@stacksjs/eslint-config",
  "type": "module",
  "version": "0.60.3",
  "description": "The Stacks.js ESLint config preset, heavily inspired by Anthony's ESLint config.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com> (https://github.com/antfu/)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/stacksjs/stacks/blob/main/storage/framework/core/eslint-config",
  "keywords": [
    "stacks",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run typegen && bunx tsup --format esm,cjs --clean --dts",
    "build:inspector": "bun run build && bunx @eslint/config-inspector build",
    "typegen": "bun scripts/typegen.ts",
    "stub": "bunx --bun tsup --format esm",
    "dev": "bunx tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "bun run stub && eslint .",
    "prepack": "bun run build",
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.59.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.2",
    "@stacksjs/cli": "workspace:*",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-flat-config-utils": "^0.2.1",
    "eslint-merge-processors": "^0.1.0",
    "eslint-parser-plain": "^0.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-jsonc": "^2.15.0",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.8.0",
    "eslint-plugin-toml": "^0.11.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest": "^0.4.1",
    "eslint-plugin-vue": "^9.24.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^15.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "toml-eslint-parser": "^0.9.3",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@stacksjs/cli": "workspace:*",
    "@stacksjs/development": "workspace:*",
    "@stacksjs/eslint-config": "workspace:*",
    "@stacksjs/utils": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^1.7.0",
    "@types/eslint": "^8.56.7",
    "@types/node": "^20.12.5",
    "@unocss/eslint-plugin": "^0.59.0",
    "eslint": "^8.57.0",
    "eslint-flat-config-viewer": "^0.1.20",
    "eslint-plugin-format": "^0.1.0",
    "eslint-typegen": "^0.2.0",
    "lint-staged": "^15.2.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0"
  }
}
