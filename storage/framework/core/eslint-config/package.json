{
  "name": "@stacksjs/eslint-config",
  "type": "module",
  "version": "0.60.3",
  "description": "The Stacks.js ESLint config preset, heavily inspired by Anthony's ESLint config.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com> (https://github.com/antfu/)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/stacksjs/stacks/blob/main/storage/framework/core/eslint-config",
  "keywords": [
    "stacks",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunx tsup --format esm,cjs --clean",
    "stub": "bunx --bun tsup --format esm",
    "dev": "bunx tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "bun run stub && eslint .",
    "prepack": "bun run build",
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.58.6",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-svelte": "^2.35.1",
    "svelte-eslint-parser": "^0.33.1"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "^1.23.0-2",
    "@eslint-types/jsdoc": "48.2.1",
    "@eslint-types/typescript-eslint": "^7.2.0",
    "@eslint-types/unicorn": "^51.0.1",
    "@stacksjs/cli": "workspace:*",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint-config-flat-gitignore": "^0.1.3",
    "eslint-merge-processors": "^0.1.0",
    "eslint-parser-plain": "^0.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-jsonc": "^2.14.1",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.7.0",
    "eslint-plugin-toml": "^0.10.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest": "^0.3.26",
    "eslint-plugin-vue": "^9.23.0",
    "eslint-plugin-yml": "^1.13.1",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^14.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "toml-eslint-parser": "^0.9.3",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@stacksjs/cli": "workspace:*",
    "@stacksjs/development": "workspace:*",
    "@stacksjs/eslint-config": "workspace:*",
    "@stacksjs/utils": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^1.7.0",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.29",
    "@unocss/eslint-plugin": "^0.58.6",
    "eslint": "^8.57.0",
    "eslint-flat-config-viewer": "^0.1.11",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  }
}
