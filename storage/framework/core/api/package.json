{
  "name": "@stacksjs/api",
  "type": "module",
  "version": "0.64.6",
  "description": "The Stacks array utilities.",
  "author": "Chris Breuer",
  "contributors": ["Chris Breuer <chris@stacksjs.org>"],
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/storage/framework/core/arrays#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./storage/framework/core/arrays"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "arrays",
    "collections",
    "contains",
    "utilities",
    "functions",
    "stacks"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "bun": "./src/*",
      "import": "./dist/*"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["README.md", "dist", "src"],
  "scripts": {
    "build": "bun build.ts",
    "generate-types": "openapi-typescript ./../../api/openapi.json --output ./../../api/api-types.ts",
    "typecheck": "bun tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@stacksjs/utils": "workspace:*",
    "ofetch": "^1.4.0",
    "openapi-typescript": "^7.4.1"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*"
  }
}
