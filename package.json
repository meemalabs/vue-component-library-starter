{
  "name": "@ow3/stacks-monorepo",
  "version": "0.23.0",
  "private": true,
  "packageManager": "pnpm@7.9.0",
  "description": "The easy way to start your component and/or function library (stack).",
  "author": "Chris Breuer",
  "license": "MIT",
  "homepage": "https://github.com/ow3org/stacks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ow3org/stacks.git"
  },
  "bugs": {
    "url": "https://github.com/ow3org/stacks/issues"
  },
  "keywords": [
    "monorepo",
    "vite",
    "component",
    "library",
    "vue",
    "web components",
    "typescript",
    "javascript",
    "starter kit"
  ],
  "sideEffects": false,
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "engines": {
    "node": ">=v16.16.0",
    "pnpm": ">=7.9.0"
  },
  "scripts": {
    "artisan": "npx artisan",
    "make:component": "npx artisan make:component",
    "make:function": "npx artisan make:function",
    "make:stack": "npx artisan make:stack",
    "fresh": "npx artisan fresh",
    "update": "npx artisan update",
    "clean": "npx artisan clean",
    "dev": "npx artisan dev",
    "dev:components": "npx artisan dev:components",
    "dev:functions": "npx artisan dev:functions",
    "dev:elements": "npx artisan dev:elements",
    "dev:docs": "npx artisan dev:docs",
    "dev:playground": "npx artisan dev:playground",
    "dev:pages": "npx artisan dev:pages",
    "dev:stacks": "npx artisan dev:stacks",
    "build": "npx artisan build",
    "build:components": "npx artisan build:components",
    "build:functions": "npx artisan build:functions",
    "build:elements": "npx artisan build:elements",
    "build:docs": "npx artisan build:docs",
    "build:playground": "npx artisan build:playground",
    "build:cli": "npx artisan build:cli",
    "build:stacks": "npx artisan build:stacks",
    "server:functions": "npx artisan server:functions",
    "server:pages": "npx artisan server:pages",
    "stubs": "npx artisan stubs",
    "commit": "npx artisan commit",
    "release": "npx artisan release",
    "changelog": "npx artisan changelog",
    "deploy:functions": "npx artisan deploy:functions",
    "deploy:docs": "npx artisan deploy:docs",
    "deploy:playground": "npx artisan deploy:playground",
    "example": "npx artisan example",
    "lint": "npx artisan lint",
    "lint:fix": "npx artisan lint:fix",
    "test": "npx artisan test",
    "test:e2e": "npx artisan test:e2e",
    "test:unit": "npx artisan test:unit",
    "test:coverage": "npx artisan test:coverage",
    "test:types": "npx artisan test:types",
    "types:fix": "npx artisan types:fix",
    "preinstall": "npx artisan preinstall",
    "postinstall": "npx artisan postinstall"
  },
  "devDependencies": {
    "@ow3/stacks": "workspace:*"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "pnpm": { 
   "peerDependencyRules": { 
     "ignoreMissing": [ 
       "@algolia/client-search", 
       "react", 
       "react-dom", 
       "@types/react" 
     ] 
   } 
 } 
}
