{
  "type": "module",
  "version": "0.23.1",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "description": "The easy way to build your component and/or function libraries & APIs.",
  "author": "Chris Breuer",
  "license": "MIT",
  "homepage": "https://github.com/ow3org/stacks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ow3org/stacks.git"
  },
  "bugs": {
    "url": "https://github.com/ow3org/stacks/issues"
  },
  "keywords": [
    "monorepo",
    "vite",
    "component",
    "composables",
    "functions",
    "ssr",
    "pwa",
    "library",
    "vue",
    "web components",
    "typescript",
    "javascript",
    "starter kit"
  ],
  "sideEffects": false,
  "bin": {
    "artisan": "./.stacks/dist/cli/artisan.mjs"
  },
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "engines": {
    "node": ">=v16.17.0",
    "pnpm": ">=7.11.0"
  },
  "scripts": {
    "artisan": "jiti ./.stacks/src/cli/index.ts",
    "fresh": "cd ./.stacks && pnpm run fresh",
    "update": "cd ./.stacks && pnpm run update",
    "dev:components": "cd ./.stacks && pnpm run dev:components",
    "dev:functions": "cd ./.stacks && pnpm run dev:functions",
    "dev:docs": "cd ./.stacks && pnpm run dev:docs",
    "build": "cd ./.stacks && pnpm run build",
    "build:components": "cd ./.stacks && pnpm run build:components",
    "build:elements": "cd ./.stacks && pnpm run build:elements",
    "build:functions": "cd ./.stacks && pnpm run build:functions",
    "build:docs": "cd ./.stacks && pnpm run build:docs",
    "build:artisan-cli": "cd ./.stacks && pnpm run build:artisan-cli",
    "lint": "cd ./.stacks && pnpm run lint",
    "lint:fix": "cd ./.stacks && pnpm run lint:fix",
    "make:component": "cd ./.stacks && pnpm run make:component",
    "make:function": "cd ./.stacks && pnpm run make:function",
    "make:lang": "cd ./.stacks && pnpm run make:lang",
    "make:stack": "cd ./.stacks && pnpm run make:stack",
    "examples:vue": "cd ./.stacks && pnpm run examples:vue",
    "commit": "cd ./.stacks && pnpm run commit",
    "release": "cd ./.stacks && pnpm run release",


    "dev:pages": "cd ./.stacks && pnpm run dev:pages",
    "build:stacks": "cd ./.stacks && pnpm run build:stacks",
    "server:functions": "cd ./.stacks && pnpm run server:functions",
    "server:pages": "cd ./.stacks && pnpm run server:pages",
    "changelog": "cd ./.stacks && pnpm run changelog",
    "deploy:functions": "cd ./.stacks && pnpm run deploy:functions",
    "deploy:docs": "cd ./.stacks && pnpm run deploy:docs",
    "deploy:playground": "cd ./.stacks && pnpm run deploy:playground",
    "test": "cd ./.stacks && pnpm run test",
    "test:e2e": "cd ./.stacks && pnpm run test:e2e",
    "test:unit": "cd ./.stacks && pnpm run test:unit",
    "test:coverage": "cd ./.stacks && pnpm run test:coverage",
    "test:types": "cd ./.stacks && pnpm run test:types",
    "types:fix": "cd ./.stacks && pnpm run types:fix",
    "preinstall": "cd ./.stacks && pnpm run preinstall",
    "postinstall": "cd ./.stacks && pnpm run postinstall"
  },
  "devDependencies": {
    "@ow3/stacks": "workspace:*"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
