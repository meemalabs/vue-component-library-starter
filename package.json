{
  "type": "module",
  "version": "0.23.0",
  "private": true,
  "packageManager": "pnpm@7.10.0",
  "description": "The easy way to start your component and/or function libraries & APIs.",
  "author": "Chris Breuer",
  "license": "MIT",
  "homepage": "https://github.com/ow3org/stacks#readme",
  "bin": {
    "artisan": "./.stacks/dist/cli/artisan.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ow3org/stacks.git"
  },
  "bugs": {
    "url": "https://github.com/ow3org/stacks/issues"
  },
  "keywords": [
    "monorepo",
    "vite",
    "component",
    "composables",
    "functions",
    "ssr",
    "pwa",
    "library",
    "vue",
    "web components",
    "typescript",
    "javascript",
    "starter kit"
  ],
  "sideEffects": false,
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "engines": {
    "node": ">=v16.17.0",
    "pnpm": ">=7.10.0"
  },
  "scripts": {
    "artisan": "jiti ./.stacks/src/cli/index.ts",
    "make:component": "pnpm artisan make:component",
    "make:function": "pnpm artisan make:function",
    "make:stack": "pnpm artisan make:stack",
    "fresh": "pnpm artisan fresh",
    "update": "pnpm artisan update",
    "clean": "pnpm artisan clean",
    "dev:components": "pnpm artisan dev:components",
    "dev:functions": "pnpm artisan dev:functions",
    "dev:docs": "pnpm artisan dev:docs",
    "dev:playground": "pnpm artisan dev:playground",
    "dev:pages": "pnpm artisan dev:pages",
    "dev:stacks": "pnpm artisan dev:stacks",
    "build": "pnpm artisan build",
    "build:components": "pnpm artisan build:components",
    "build:functions": "pnpm artisan build:functions",
    "build:elements": "pnpm artisan build:elements",
    "build:docs": "pnpm artisan build:docs",
    "build:playground": "pnpm artisan build:playground",
    "build:cli": "pnpm artisan build:cli",
    "build:stacks": "pnpm artisan build:stacks",
    "server:functions": "pnpm artisan server:functions",
    "server:pages": "pnpm artisan server:pages",
    "stubs": "pnpm artisan stubs",
    "commit": "pnpm artisan commit",
    "release": "pnpm artisan release",
    "changelog": "pnpm artisan changelog",
    "deploy:functions": "pnpm artisan deploy:functions",
    "deploy:docs": "pnpm artisan deploy:docs",
    "deploy:playground": "pnpm artisan deploy:playground",
    "example": "pnpm artisan example",
    "lint": "pnpm artisan lint",
    "lint:fix": "pnpm artisan lint:fix",
    "test": "pnpm artisan test",
    "test:e2e": "pnpm artisan test:e2e",
    "test:unit": "pnpm artisan test:unit",
    "test:coverage": "pnpm artisan test:coverage",
    "test:types": "pnpm artisan test:types",
    "types:fix": "pnpm artisan types:fix",
    "preinstall": "pnpm artisan preinstall",
    "postinstall": "pnpm artisan postinstall"
  },
  "devDependencies": {
    "@ow3/stacks": "workspace:*"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "pnpm": { 
   "peerDependencyRules": { 
     "ignoreMissing": [ 
       "@algolia/client-search", 
       "react", 
       "react-dom", 
       "@types/react" 
     ] 
   } 
 } 
}
